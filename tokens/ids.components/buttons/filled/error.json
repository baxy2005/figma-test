{
  "ids": {
    "comp": {
      "button": {
        "filled": {
          "color": {
            "error": {
              "enabled-bg": {
                "value": "{ids.light.color.selectable.error.error-default}",
                "type": "color"
              },
              "hovered-bg": {
                "value": "{ids.comp.button.filled.color.error.enabled-bg}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "mix",
                      "value": "{ids.smc.color.modifiers.button.filled.hovered-lightness}",
                      "space": "hsl",
                      "color": "{ids.light.color.states.filled}"
                    }
                  }
                }
              },
              "focused-bg": {
                "value": "{ids.comp.button.filled.color.error.enabled-bg}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "mix",
                      "value": "{ids.smc.color.modifiers.button.filled.focused-lightness} ",
                      "space": "hsl",
                      "color": "\n{ids.light.color.states.filled}"
                    }
                  }
                }
              },
              "pressed-bg": {
                "value": "{ids.comp.button.filled.color.error.enabled-bg}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "mix",
                      "value": "{ids.smc.color.modifiers.button.filled.pressed-lightness} ",
                      "space": "hsl",
                      "color": "{ids.light.color.states.filled-inverse}"
                    }
                  }
                }
              },
              "on-enabled": {
                "value": "{ids.light.color.selectable.error.on-error}",
                "type": "color"
              }
            }
          }
        }
      }
    }
  }
}