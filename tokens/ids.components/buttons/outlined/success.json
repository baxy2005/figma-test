{
  "ids": {
    "comp": {
      "button": {
        "outlined": {
          "color": {
            "success": {
              "enabled-bg": {
                "value": "{ids.base.color.white}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{ids.smc.color.modifiers.alpha.transparent}",
                      "space": "hsl"
                    }
                  }
                }
              },
              "outline": {
                "value": "{ids.light.selectable.success.success-default}",
                "type": "color"
              },
              "hovered-bg": {
                "value": "{ids.light.selectable.success.success-default}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{ids.smc.color.modifiers.button.outline.hovered-lightness} ",
                      "space": "hsl"
                    }
                  }
                }
              },
              "focused-bg": {
                "value": "{ids.light.selectable.success.success-default}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{ids.smc.color.modifiers.button.outline.focused-lightness} ",
                      "space": "hsl"
                    }
                  }
                }
              },
              "pressed-bg": {
                "value": "{ids.light.selectable.success.success-default}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{ids.smc.color.modifiers.button.outline.pressed-lightness} ",
                      "space": "hsl"
                    }
                  }
                }
              },
              "on-enabled": {
                "value": "{ids.light.selectable.success.on-success-inverse}",
                "type": "color"
              }
            }
          }
        }
      }
    }
  }
}