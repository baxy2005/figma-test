{
  "ids": {
    "comp": {
      "button": {
        "error": {
          "color": {
            "text": {
              "enabled-bg": {
                "value": "{ids.base.color.white}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{ids.smc.color.modifiers.alpha.transparent}",
                      "space": "lch"
                    }
                  }
                }
              },
              "hovered-bg": {
                "value": "{ids.smc.color.selectable.error.on-error-inverse}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{ids.smc.color.modifiers.button.text.hovered-lightness} ",
                      "space": "hsl"
                    }
                  }
                }
              },
              "focused-bg": {
                "value": "{ids.smc.color.selectable.error.on-error-inverse}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{ids.smc.color.modifiers.button.text.focused-lightness} ",
                      "space": "hsl"
                    }
                  }
                }
              },
              "pressed-bg": {
                "value": "{ids.smc.color.selectable.error.on-error-inverse}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{ids.smc.color.modifiers.button.text.pressed-lightness} ",
                      "space": "lch"
                    }
                  }
                }
              },
              "disable-bg": {
                "value": "{ids.smc.color.page.surface.surface-inverese}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{ids.smc.color.modifiers.alpha.disabled}",
                      "space": "hsl"
                    }
                  }
                }
              },
              "on-enabled": {
                "value": "{ids.smc.color.selectable.error.on-error-inverse}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "darken",
                      "value": "{ids.smc.color.modifiers.button.text.on-inverse}",
                      "space": "hsl"
                    }
                  }
                }
              },
              "on-disabled": {
                "value": "{ids.smc.color.page.surface.on-surface}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{ids.smc.color.modifiers.alpha.on-disabled}",
                      "space": "lch"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}